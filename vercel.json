{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/apps/server/src/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/apps/web/$1"
    }
  ],
  "functions": {
    "apps/server/src/index.ts": {
      "runtime": "vercel-bun@1.3.6"
    }
  }
}
